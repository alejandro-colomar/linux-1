Vagrant.configure("2") do |config|
  config.vm.define "w0" do |m|
    m.vm.provider "virtualbox" do |v|
      v.name = "swarm_w0"
      v.linked_clone = true
    end
    m.vm.box = "centos/7"
    m.vm.network "private_network",
      virtualbox__intnet: "swarm",
      nic_type: "virtio"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.network.w0"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.docker"
  end
  config.vm.define "w1" do |m|
    m.vm.provider "virtualbox" do |v|
      v.name = "swarm_w1"
      v.linked_clone = true
    end
    m.vm.box = "centos/7"
    m.vm.network "private_network",
      virtualbox__intnet: "swarm",
      nic_type: "virtio"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.network.w1"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.docker"
  end
  config.vm.define "w2" do |m|
    m.vm.provider "virtualbox" do |v|
      v.name = "swarm_w2"
      v.linked_clone = true
    end
    m.vm.box = "centos/7"
    m.vm.network "private_network",
      virtualbox__intnet: "swarm",
      nic_type: "virtio"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.network.w2"
    m.vm.provision "shell",
      path: "https://raw.githubusercontent.com/secobau/linux/master/Vagrant/Swarm/Wx/setup.docker"
  end
