vagrant ssh -c "echo ${MY_XWIKI_USER:-xwiki} | docker secret create xwiki-db-username -"
vagrant ssh -c "echo $(cat /dev/urandom | tr -dc 'a-zA-Z0-9' | fold -w 16 | head -n 1) | docker secret create xwiki-db-password -"
vagrant ssh -c "echo $(cat /dev/urandom | tr -dc 'a-zA-Z0-9' | fold -w 16 | head -n 1) | docker secret create xwiki-db-root-password -"
