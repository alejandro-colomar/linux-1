#!/bin/sh
token_do=$(cat token_do);
for x in w2X;do
 mkdir $x;
 cd $x;
 sed s/token_do/$token_do/ Vagrantfile.pre 1>Vagrantfile;
 sed -i s/location/$location/ Vagrantfile;
 sed -i s/SSH_KEY_NAME/$SSH_KEY_NAME/ Vagrantfile;
 sed -i "s/\.dc/.$dc/" Vagrantfile;
 vagrant up --parallel;
 cd -;
 done
cd m0;
cd -;
